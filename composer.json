{
    "name": "eolica/laravel-content-tools",
    "description": "A Laravel package for editing your web frontend translations directly on the page",
    "keywords": [
        "eolica",
        "laravel-content-tools",
        "laravel",
        "content",
        "content tools",
        "translations",
        "frontend",
        "frontend translations",
        "page translations"
    ],
    "homepage": "https://github.com/Eolica-Web/laravel-content-tools",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "David Llobell",
            "email": "dllobell@eolicadigital.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "brick/varexporter": "^0.3.7",
        "illuminate/contracts": "^8.0 || ^9.0",
        "illuminate/filesystem": "^8.0 || ^9.0",
        "illuminate/http": "^8.0 || ^9.0",
        "illuminate/support": "^8.0 || ^9.0",
        "illuminate/view": "^8.0 || ^9.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0 || ^7.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Eolica\\LaravelContentTools\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Eolica\\LaravelContentTools\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Eolica\\LaravelContentTools\\ContentToolsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
